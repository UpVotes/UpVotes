@if (ViewBag.isPublicQuote)
{  
    <meta property='og:title' content="How Much Does it Cost to Make an Mobile App in @DateTime.Now.Year? | App Cost Calculator | Upvotes.co" />
    <meta name='description' content="Find out how much does mobile app development cost for iOS or Android? We've created this handy mobile app cost calculator for make approximate cost." />
}
<div id="divPublic">
    @Scripts.Render("~/bundles/Quotation")    
    @Styles.Render("~/Content/Quotationcss")
    
    
    @if (ViewBag.isPublicQuote)
